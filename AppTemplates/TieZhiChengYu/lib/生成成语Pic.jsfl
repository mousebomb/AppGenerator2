	var doc=fl.getDocumentDOM(); var lib = fl.getDocumentDOM().library; //library 对象   var timu = "天长地久日久天长打情骂俏三心二意刀山火海";var len = timu.length;for(var i = 0;i<len ; i++){		var itemName = (i+1)+"_"+ timu[i];	if ( lib.itemExists(itemName))  lib.deleteItem(itemName);	var picItemName = "Pic"+(i+1);	if(lib.itemExists(picItemName)) lib.deleteItem(picItemName);	doc.addNewText({left:0, top:0, right:88, bottom:88} ,timu[i]); 	var tx=doc.getTimeline().layers[0].frames[0].elements[0];//选择文本	tx.textType="static";	doc.selectAll();  	doc.breakApart();	doc.convertToSymbol("movie clip",itemName,"center") ; //将文本转化为mc元件     	 lib.selectNone();     doc.selectAll(); //清空舞台   		// 制作PicXdoc.convertToSymbol("movie clip",picItemName,"center"); lib.selectItem(picItemName);	 lib.setItemProperty('linkageExportForAS', true);        lib.setItemProperty('linkageExportForRS', false);        lib.setItemProperty('linkageExportInFirstFrame', true);        lib.setItemProperty('linkageClassName', picItemName);         lib.setItemProperty('scalingGrid',  false); 	 lib.setItemProperty('linkageBaseClass',  "flash.display.MovieClip"); 	      doc.selectAll(); //清空舞台        doc.deleteSelection();	 	 //加涂层	 lib.editItem(picItemName);	 doc.selectAll();	 	 doc.setInstanceTint(0x000000,100);	 	 //	 doc.getTimeline().insertKeyframe(5);	 doc.selectAll();	 doc.setInstanceTint(0,0);	 doc.getTimeline().insertFrames(5);	 doc.getTimeline().addNewLayer();	 doc.getTimeline().layers[0].frames[0].name="black";	 doc.getTimeline().insertKeyframe(5);	 doc.getTimeline().layers[0].frames[5].name="show";	 	 lib.selectNone();	 	 doc.selectNone();	 doc.editScene(0);}